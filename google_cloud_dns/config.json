{
  "name": "Google Cloud DNS",
  "version": "1.0.1",
  "slug": "google_cloud_dns",
  "description": "Google Cloud DNS service with Let's Encrypt support",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "services",
  "stage": "experimental",
  "map": ["config", "ssl:rw"],
  "options": {
    "lets_encrypt": {
      "accept_terms": false,
      "certfile": "fullchain.pem",
      "keyfile": "privkey.pem",
      "renewal_period": 5184000,
      "dns_delay": 60
    },
    "project": null,
    "zone": null,
    "email": null,
    "keyfile": "gcp-account.p12",
    "keyfile_password": "notasecret",
    "domain": null,
    "ttl": 600,
    "scan_interval": 300
  },
  "schema": {
    "lets_encrypt": {
      "accept_terms": "bool",
      "certfile": "str",
      "keyfile": "str",
      "renewal_period": "int",
      "dns_delay": "int"
    },
    "ipv4": "str?",
    "ipv6": "str?",
    "project": "str",
    "zone": "str",
    "email": "email",
    "keyfile": "str",
    "keyfile_password": "str",
    "domain": "str",
    "hostname": "str?",
    "ttl": "int",
    "scan_interval": "int"
  }
}
